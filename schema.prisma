model Task @notionDatabase("aac810fcb3414dbb9c46ce485bc6449b") {
  name              String    @title @map("Name")
  completed         Boolean?  @checkbox @map("完了")
  emphasized        Boolean?  @checkbox @map("注力")
  subItems          String[]  @relation @map("Sub-item")
  relatedPages      String[]  @relation @map("似てるページ")
  date              String?   @map("日付")
  manager           Json?     @map("責任者")
  archived          Boolean?  @checkbox @map("アーカイブ")
  timeline          String[]  @relation @map("OYKOT Timeline")
  action            String[]  @relation @map("Action")
  objectiveProgress String?   @formula @map("Objective進行度")
  parentItem        String[]  @relation @map("Parent item")
  actionProgress    String?   @formula @map("Action進行度")
}