model Task @notionDatabase("***REMOVED***") {
  name        String    @title @map("Name")
  date        Date?     @map("日付")
  completed   Boolean   @checkbox @map("完了")
  emphasized  Boolean   @checkbox @map("注力")
  archived    Boolean   @checkbox @map("アーカイブ")
  manager     People?   @map("責任者")
}

model Project @notionDatabase("notion-database-id-2") {
  name        String    @title
  description String
  startDate   Date
  budget      Number
  status      String
}