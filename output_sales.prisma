generator client {
  provider = "notion-orm"
  output   = "./src/generated"
  types    = "notionTypes.ts"
  client   = "notionClient.ts"
}

model Deal { 
// id: 1d060c3cd57781e59422c99b20e39127
  companyGroupName String    @map("社名・グループ名") @title
  updateRecord     DateTime? @map("記載更新") @date
  priorityMonth    String?   @map("優先度（商談月）") @select
  progress         String?   @map("案件進捗") @select
  winProbability   String?   @map("受注確度") @select
  salesRep         Json?     @map("担当営業") @people
  industry         String?   @map("業界") @select
  newGrad          Boolean?  @map("新卒") @checkbox
  midCareer        Boolean?  @map("中途") @checkbox
  annualHiring     Int?      @map("年間採用人数")
  newGradScale     Int?      @map("新卒採用人数規模")
  midCareerScale   Int?      @map("中途採用人数規模")
  connection       String?   @map("コネクション") @select
  connectionSource String?   @map("コネクション元") @select
  remarks          String?   @map("備考") @richText
  revenuePotential String?   @map("売上ポテンシャル")
  expectedOrder    String?   @map("受注金紙見込み")
}
